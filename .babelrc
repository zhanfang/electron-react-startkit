{
    "comments": false,
    "env": {
        "main": {
            "presets": [
            ["env", {
                "targets": { "node": 7 }
            }],
            "stage-0"
            ]
        },
        "renderer": {
            "presets": [
            ["env", {
                "modules": false
            }],
            "stage-0",
            "react"
            ],
            "plugins": [
                "transform-decorators-legacy"
            ]
        },
        "web": {
            "presets": [
            ["env", {
                "modules": false
            }],
            "stage-0",
            "react"
            ],
            "plugins": [
                "transform-decorators-legacy"
            ]
        }
    }
}
  